{
    "name": "pricone-tl-highlight",
    "displayName": "Pricone-TL-highlight",
    "description": "Princess Connect Re:Dive のクランバトルで使用するタイムラインをハイライトする拡張機能",
	"publisher": "hibi",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tl",
            "aliases": ["pricone-tl", "tl"],
            "extensions": [".tl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tl",
            "scopeName": "source.tl",
            "path": "./syntaxes/tl.tmLanguage.json"
        }]
    }
}