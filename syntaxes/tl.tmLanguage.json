{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "pricone-tl",
	"patterns": [
		{
			"include": "#nyaru"
		},
		{
			"include": "#bossub"
		},
		{
			"include": "#kokkoro"
		},
		{
			"include": "#chris"
		},
		{
			"include": "#maho"
		},
		{
			"include": "#rei"
		},
		{
			"include": "#akino"
		},
		{
			"include": "#puyori"
		}
	],
	"repository": {
		"nyaru": {
			"patterns": [{
				"name": "token.debug-token",
				"match": "キャル\\s?[\\(（]ニューイヤー[\\)）]|ニャル"
			}
			]
		},
		"bossub":{
			"patterns": [
				{
					"name":"invalid.illegal",
					"match": ".*(敵UB|ボスUB|bossUB).*"
				}
			]
		},
		"kokkoro":{
			"patterns": [
				{
					"name": "entity.name.class",
					"match": "コッコロ\\s?[\\(（]プリンセス[）\\)]|コッコロ|プリコロ"
				}
			]
		},
		"chris":{
			"patterns":[
				{
					"name": "entity.other.attribute-name.class.css",
					"match": "[^(クリ)]クリス(ティーナ)?"
				}
			]
		},
		"kasumi":{
			"patterns": [
				{
					"name": "keyword.operator.new",
					"match": "カスミ"
				}
			]
		},
		"rei":{
			"patterns": [
				{
					"name": "constant.character",
					"match": "\\sレイ"
				}
			]
		},
		"maho":{
			"patterns": [
				{
					"name": "meta.preprocessor.string",
					"match": "マホ"
				}
			]
		},
		"akino":{
			"patterns": [
				{
					"name": "token.debug-token",
					"match": "アキノ"
				}
			]
		},
		"puyori":{
			"patterns": [
				{
					"name": "variable",
					"match": "プヨリ|ヒヨリ\\s?[\\(（]プリンセス[）\\)]"
				}
			]
		}
	},
	"scopeName": "source.tl"
}